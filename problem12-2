print("Problem 12-2\n")

depth1 = int(input("Enter Depth 1:"))
depth2 = int(input("Enter Depth 2:"))
depth3 = int(input("Enter Depth 3:"))
depth4 = int(input("Enter Depth 4:"))

if depth1 == depth2 == depth3 == depth4:
    print("Fish At Constant Depth")

elif depth1 < depth3 < depth2 < depth4:
    print("Fish Diving")

elif depth1 > depth2 > depth3 > depth4:
    print("Fish Rising")

else:
    print("No Fish")
